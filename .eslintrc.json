{
    "env": {
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "@typescript-eslint/method-signature-style": "off",
        "@typescript-eslint/prefer-readonly": "off",
        "@typescript-eslint/no-misused-promises": "off"
    }
}
